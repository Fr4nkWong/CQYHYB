<div class="col-sm-9 col-sm-offset-3 col-md-11 col-md-offset-1 main">

  <h2 class="sub-header">文档添加</h2>
  <form method="POST" action="/manager/doc_add" class="form-horizontal col-md-5 col-md-offset-2" role="form" enctype='multipart/form-data'>
      <div class="form-group">
        <label class="control-label col-sm-3" for="doc_id">编号：</label>
        <div class="col-sm-9">
          <input type="text" class="form-control" id="doc_id" name="doc_id" />
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="doc_type">类型：</label>
        <div class="col-sm-9">
          <select class="form-control" id="doc_type" name="doc_type">
            <option value="产品资料">产品资料</option>
            <option value="产品说明">产品说明</option>
            <option value="产品明细">产品明细</option>
          </select>
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="doc_upload">文件上传：</label>
        <div class="col-sm-9">
          <input type="file" id="doc_upload" name="doc_upload" multiple='mutiple' />
        </div>
      </div>
      <div class="row">
        <button type="reset" class="btn btn-default col-sm-1 col-sm-offset-6" id="reset">重置</button>
        <button type="submit" class="btn btn-default col-sm-1 col-sm-offset-1" id="submit">提交</button>
      </div>
    </form>

</div>