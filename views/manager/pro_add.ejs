<div class="col-sm-9 col-sm-offset-3 col-md-11 col-md-offset-1 main">

  <h2 class="sub-header">产品添加</h2>
  <form method="POST" action="/manager/pro_add" class="form-horizontal col-md-5 col-md-offset-2" role="form" enctype="multipart/form-data">
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_id">编号：</label>
        <div class="col-sm-9">
          <input type="text" class="form-control" id="pro_id" name="pro_id" />
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_name">名称：</label>
        <div class="col-sm-9">
          <input type="text" class="form-control" id="pro_name" name="pro_name"/>
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_type1">一级类型：</label>
        <div class="col-sm-9">
          <select class="form-control" id="pro_type1" name="pro_type1">
            <option value="温度仪表">温度仪表</option>
            <option value="压力仪表">压力仪表</option>
            <option value="流量仪表">流量仪表</option>
            <option value="物位仪表">物位仪表</option>
            <option value="工作自动化仪表">工作自动化仪表</option>
            <option value="仪表管阀件">仪表管阀件</option>
          </select>
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_type2">二级类型：</label>
        <div class="col-sm-9">
          <input type="text" class="form-control" id="pro_type2" name="pro_type2"/>
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_pic">产品图片：</label>
        <div class="col-sm-9">
          <input type="file" id="pro_pic" name="pro_pic"/>
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_pic">补充图片：</label>
        <div class="col-sm-9">
          <input type="file" id="pro_pic1" name="pro_pic1" multiple="multiple"/>
        </div>
        <div class="col-sm-9">
          <input type="file" id="pro_pic2" name="pro_pic2" multiple="multiple"/>
        </div>
      </div>
      <div class="form-group">
        <label class="control-label col-sm-3" for="pro_info">说明：</label>
        <div class="col-sm-9">
          <textarea class="form-control" rows="19" name="pro_info" id="pro_info"></textarea>
        </div>
      </div>
      <div class="row">
        <button type="reset" class="btn btn-default col-sm-1 col-sm-offset-6" id="reset">重置</button>
        <button type="submit" class="btn btn-default col-sm-1 col-sm-offset-1" id="submit">提交</button>
      </div>
    </form>

</div>